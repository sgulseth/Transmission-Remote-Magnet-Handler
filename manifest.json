{
  "name": "Transmission Remote Magnet Handler",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Adds magnet-links to transmission-remote.",
  "options_page": "options.html",
  "browser_action": {
    "default_icon": "icon.png"
  },
   "content_scripts": [
    {
      "matches": ["http://*/*"],
      "js" : ["jquery.min.js","config.js","script.js"]
    }
  ]
}